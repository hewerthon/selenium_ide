    suite.addTestSuite(Sign-SB-F-HM.class);
    suite.addTestSuite(Sign-SB-F-M.class);
    suite.addTestSuite(Sign-SB-M-H.class);
    suite.addTestSuite(Sign-SB-M-HM.class);
    suite.addTestSuite(Sign-SB-M-M.class);
    suite.addTestSuite(Sign-SD-H.class);
    suite.addTestSuite(Sign-SD-M.class);
    suite.addTestSuite(Sign-SD-MH.class);
    suite.addTestSuite(Sign-SM-H.class);
    suite.addTestSuite(Sign-SM-M.class);
    suite.addTestSuite(Sign-SM-MH.class);
    suite.addTestSuite(Delete Fotos.class);
    suite.addTestSuite(Finance.class);
    suite.addTestSuite(Testes de comandos.class);
    suite.addTestSuite(Delete MSG.class);
